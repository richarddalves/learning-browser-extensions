{
    "manifest_version": 3,
    "name": "Reading Time",
    "description": "This is my first time creating manifest by myself",
    "version": "1.0.0",
    "permissions": ["scripting", "activeTab"],
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["content.js"]
        }
    ]
}